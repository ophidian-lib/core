{
  "name": "@ophidian/core",
  "version": "0.0.24",
  "description": "A Component framework for Obsidian Plugins",
  "keywords": [
    "obsidian"
  ],
  "repository": "https://github.com/ophidian-lib/core.git",
  "author": "PJ Eby",
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "./dist/*", "./src/augments.d.ts"
  ],
  "sideEffects": false,
  "packageManager": "pnpm@7.33.6",
  "dependencies": {
    "defaults": "^2.0.1",
    "i18next": "^20.3.2",
    "monkey-around": "^3",
    "obsidian": "1.3.5",
    "to-use": "^0.3.3"
  },
  "devDependencies": {
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.3",
    "@knodes/typedoc-plugin-pages": "^0.23.4",
    "@knodes/typedoc-pluginutils": "^0.23.4",
    "@types/node": "16",
    "esbuild-plugin-inline-import": "^1.0.4",
    "onchange": "^7.1.0",
    "sass": "^1.76.0",
    "tsup": "^8.0.2",
    "tsx": "^4.10.0",
    "typedoc": "^0.25.13",
    "typescript": "5.2.2",
    "uneventful": "^0.0.10"
  },
  "peerDependencies": {
    "uneventful": "^0.0.10"
  },
  "scripts": {
    "docs": "tsx typedoc/run.mts",
    "watch-docs": "onchange -i --kill \"*.md\"  \"guides/**/*.md\" \"typedoc.config.*\" \"typedoc/*\" -- tsx typedoc/run.mts --watch",
    "watch-prepare": "tsup --watch",
    "prepare": "tsup --clean"
  }
}
